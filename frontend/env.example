# Переменные окружения для фронтенда
# Скопируйте этот файл в .env.local и заполните значения

# URL бэкенда API (для продакшена укажите ваш домен)
VITE_API_URL=https://your-backend-api.com

# Дополнительные настройки
VITE_APP_NAME=AI Competitor Insight Hub
VITE_APP_VERSION=0.1.0

# Настройки для разработки (не используются в продакшене)
# Основной способ — переопределить VITE_API_URL в .env.local.
# Дополнительно можно указать fallback, который сработает только если VITE_API_URL не задан.
VITE_DEV_API_URL=http://localhost:8000
